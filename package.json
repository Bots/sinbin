{
  "name": "sinbin",
  "version": "1.1.0",
  "description": "Real-time swear jar overlay for Twitch streaming with speech recognition",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/server.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "twitch",
    "overlay",
    "speech-recognition",
    "streaming",
    "obs"
  ],
  "author": "John Paul Wile (BotsOne)",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^6.0.1",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "node-record-lpcm16": "^1.0.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
